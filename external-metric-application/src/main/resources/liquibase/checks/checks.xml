<!--    </changeSet>-->

    <changeSet id="check_postfix_constraint.sql" author="vood" runAlways="true">
        <validCheckSum>any</validCheckSum>
        <sqlFile path="liquibase/checks/check_postfix_constraint.sql" endDelimiter="${sqlBatchDelimiter}"/>
    </changeSet>

</databaseChangeLog>